# Required: OpenAI API key for the purple agent (paper_agent.py)
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Optional: LLM model identifier for the purple agent
# Default: "openai/gpt-4.1-mini"
# Examples: "openai/gpt-4o-mini", "anthropic/claude-3-haiku", "google/gemini-pro"
PURPLE_MODEL=openai/gpt-4o-mini

# Optional: Google API configuration (if using Google models via litellm)
# GOOGLE_GENAI_USE_VERTEXAI=TRUE
# GOOGLE_API_KEY=your_google_api_key_here
